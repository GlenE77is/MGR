#(: ==========================================================================
#(: fx Sys_Case_PupMode #! ACTIVE   
#(: called by: 
#(: param:
#(: purpose: 
#(: calls:
#(: method: 
#(: sends:
#(: rationale:
#(: note :
#! #################################################################################
function Sys_Case_PupMode() {    #! ACTIVE  #! temp: not called , position problem. 
#! --- Frugal only --- first possible check
case $PUPMODE in
    12|13) ok=1 ;;  
    *) ERRORMSG="only (12|13)" 
        sleep 3
            #!exit 1
            Sbr_Cbx_ReQuit 	#! must follow the Called Sbr_Cbx_ReQuit . 
        ;;
esac
} #! end Sys_Case_PupMode   
#!
#(: ==========================================================================
#(: fx Sys_CalcRamObj #! ACTIVE   
#(: called by: 
#(: param:
#(: purpose: 
#(: calls:
#(: method: 
#(: sends:
#(: rationale:
#(: note :
#! #################################################################################
function Sys_CalcRamObj() {     #! req modularity
    #! "top=RamObj ==============================================="
    #! dev: "calc" if [ zero ]  for "${gcPupRamObjMsg}"
    if [ "${gcPupRamObjMsg}" ] ; then  # NOT ZERO   # do nothing
         n="0"
        #! MGR has been run/initialized the "first" time. So, Will Show "RamObjMsg" Col #1, bottom. 
        sleep .1    
    else    # so must generate . 
    #! "gen=RamObj ==============================================="
        #! ------------------------------------
        aa="$(du -c -m -s ${lcPupSaveData} )"     # error flag on Zero-length file shown inside Geany/XTerm
        read -r b ccc <<< "$aa" # --- saveDATA size calc
        #~   -r b ccc <<< "$aa" # --- saveDATA size calc
        #!
        export gcPupRamObjMsg="${b} MB "
        #!
        ### changed from leading grave [`] to enclosing double-apostrophe ["]
        ### FREERAM="free | grep -o 'Mem: .*' | tr -s ' ' | cut -f 4 -d ' '"
        ### export gcFreeRamNow="${FREERAM}"
        ### export gcFreeRamData="# Ram: ${gcFreeRamNow} Bytes "
    fi
    #! "bot=RamObj ==============================================="
} # end:  Sys_CalcRamObj
