#! #####################################################################
#! pup_Trx64_SDA	    Legacy  
#! PSUBDIR='/pup_Trx64_SDA'
#!	DISTRO_NAME='   '
#!  DISTRO_FILE_PREFIX='trixiepup64'

    if [ "${DISTRO_NAME}" = 'TrixiePup64' ] ||  [ "${DISTRO_FILE_PREFIX}" = 'trixiepup64' ] ; then
            export gcMgrDistroSymbol="={Trx-64}="  #! on system screen 

			export gcSysChkBtnFlg="true"		#! <button> visible=" " 	trixiepup64
            export gcSysChkMnxFlg="true"		#! <menu> visible=" "   	trixiepup64
            export gcSysChkPexFlg="false"		#! <Pex> this is PEX OS     trixiepup64
            export gcSysChkTtlFlg="false"		#! <Ttl> Title is HomoGeneous

			export gcMgrIsoDate="iso_TrixiePup64_Legacy-11.1.1_251028"   #!
			export gcMgrPuppyDate="${daedalus}"

