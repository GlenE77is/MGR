            #~ export gcDistroPuppyDate="250500"	#! DISTRO_PUPPYDATE='May 2025'"
            gcMrgAdjOut="true"  #! deprecated &&  gcMrgAdjL="true"  &&  gcMrgRAdj="true"   
            gcMrgBit32="0"      
