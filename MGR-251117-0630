			export gcMgrPuppyDate="${daedalus}"
	fi
    #!


#! ### PEX ### Special for PupEXtron ####################################
#! pup_PEX-N64-SDA
#! PSUBDIR='/pup_PEX-N64-SDA'
#!	!!! PEX-N64 does not have a DISTRO file !
#! must test zero and assign a default to catch PEX-N64
#! but, N64 is OK with default screen settings. 

#! gcSysDistroName is empty , because PEX_N64 has no DISTRO 
#! PUPSTATE has this:
#! PSUBDIR='/pup_PEX-Dev64-SDA'
#! DISTRO_NAME='NoblePup64'

			#! Iso="iso_NoblePup32-24.04-241101"   
			#! PSUBDIR='/pup_PEX-N64-SDA'
			#! * DISTRO_NAME='puppex_dev'
			#! DISTRO_VERSION=13.0
			#! DISTRO_BINARY_COMPAT='devuan'
			#! DISTRO_FILE_PREFIX='puppex'
			#! * DISTRO_COMPAT_VERSION='daedalus'

			#!"PEX-N64-SDA")	#! PEX Nobel 64   #! PEX-N640
        #!
#! ### N64N ### Noble64 ###############################################################
	if  [ "${DISTRO_NAME}" = "NoblePup64" ]  ; then 
            export gcMgrDistroSymbol="={N64}="  #! on system screen 
	
			export gcSysChkBtnFlg="true"		#! <button> visible=" " NoblePup64
            export gcSysChkMnxFlg="true"		#! <menu> visible=" "   NoblePup64
            export gcSysChkPexFlg="false"		#! <Pex>  is PEX OS     NoblePup64
            export gcSysChkTtlFlg="false"		#! <Ttl> Title is HomoGeneous

            export gcMrgBit32="1" 
            export gcMrgGuiWid="1100"   
            
			 export gcMgrIsoDate="240401"   #!
			 export gcMgrPuppyDate="${DISTRO_PUPPYDATE}"
	fi
    #!
#! ### PEX_N64 ##################################################################
	if  [ "${DISTRO_NAME}" = "PEX_NoblePup64" ]  ; then 
            export gcMgrDistroSymbol="={PEX_N64}="  #! on system screen 
	
			export gcSysChkBtnFlg="true"		#! <button> visible=" " NoblePup64
            export gcSysChkMnxFlg="true"		#! <menu> visible=" "   NoblePup64
            export gcSysChkPexFlg="true"		#! <Pex>  is PEX OS     NoblePup64
            export gcSysChkTtlFlg="false"		#! <Ttl> Title is HomoGeneous

            export gcMrgBit32="1" 
            export gcMrgGuiWid="1100"   
            
