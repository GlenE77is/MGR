                Sbr_FnNotFound "${lcFnRun}"
            fi
    Sbr_Cbx_ReStart 1    
        EXIT="" ### force "Fall Thru" if BASH call fails
        exit 1      #! ## "exit 1' , error , should terminate this program quietly
fi # end: EXIT:guiCTB52
