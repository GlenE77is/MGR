#(: begin: ( EXIT:'CBX APPLY+')
#(: begin: check for "APPLY Checks" command   Check-Box "CBX"  # External calls
#(: fx EXIT='APPLY_Checks_All_'
#(: fx EXIT='CBXAR3')
#(: fx EXIT='CBXARS'
#(: fx EXIT='CBXARB'
#(: fx EXIT='CBXAPO'
