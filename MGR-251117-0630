#! ############################################################
#!
#!
#!
#!
#! ###_CBX_################################################
#! BEGIN:     CBx   Combo-Boxes   #! Not Active
#! ###_CBX_################################################
#!
#(: =====================================================================
#(: fx Sbr_ClearFlagsSystem   #: Active for System
#(: called by:
#(: param:
#(: purpose: clear all system flags
#(: calls:
#(: method:
#(: sends:
#(: rationale:
#(: note : some flags are still hard-code ; ie, LockFile , BackUpStatus
#(: preceded by call to Sbr_CBX_ReStart and $0
#! ############################################################
function Sbr_ClearFlagsSystem() {   # ACTIVE    # at 3560
    #! for System,  lingering "clear" for old var , to be culled. 
    n=0
    #! current in-use
    rm -f "${gcMGR_LockFile}"             2>&1>/dev/null  ### this is "MGR_LockFile" ###  but not E3R  CBX
    #~ rm -f "${MGR_BackUpStatus}"         2>&1>/dev/null 
    #~ rm -f "${MGR_ReStart_flg}"          2>&1>/dev/null  ### MGR initial Re-ReStart to init GEANY   # Sbr_ClearFlagsQuit
    #! tmp usage
    rm -f "/tmp/snapmergepuppy-error"   2>&1>/dev/null  ### this is SMP flag , via SMP
    rm -f "/tmp/flagnextpassthru"       2>&1>/dev/null  ### used by Linux , ? via SMP 
    rm -f "/tmp/box_help"               2>&1>/dev/null  ### remove the lingering temp file.
    rm -f "/tmp/box_source"             2>&1>/dev/null  ### remove the lingering temp file.
    rm -f "/tmp/EmGa-Master-GUI.txt"          2>&1>/dev/null    ### Embedded Image Flag file
    #~ rm -f "/tmp/gtkdialog_splash_border.svg"  2>&1>/dev/null 
    #~ rm -f "/tmp/gtkdialog_splash_bg.svg"      2>&1>/dev/null 
    #~ rm -f "/tmp/gtkdialog_splash_bg.svg"      2>&1>/dev/null 
    #~ rm -f "/tmp/Mgr_Dump_Path.txt"            2>&1>/dev/null 
    #~ rm -f "/tmp/Mgr_Dump_Declare.txt"         2>&1>/dev/null 
    gcMGRConfigArbIdd=""            #! "menu config"  
    /bin/echo " " > /dev/console
} #
export -f Sbr_ClearFlagsSystem
#!
#!
#!
#!
#(: ======================================================
#(: fx Sbr_Cbx_ReStart
#(: called by:  # CBX_ReStart et al
#(: param:
#(: purpose: ReStart $0 , fresh !
#(: calls:
#(: called by: most <button> CBX, ARB, E3R
#(: method:
#(: sends:
#(: rationale:
#(: note : called by ROX
#(:
#! ###_CBX_###  "Sbr_Cbx_Re" ##############################################
function Sbr_Cbx_ReStart() {   # ACTIVE   Group:  "Sbr_Cbx_Re"
    #! for Check-Box ARB_ReStart do a simple 'warm' restart ${0}
    n=0
    lcMsgStr="ReStart"
    Sbr_ClearFlagsSystem  ""   
    sleep 1
    clear   #! Xterm debugging screen 
    #! This is "ReStart"   
    . ${0}      #: $ ZERO   # will run $0 / ReStart fresh .
    #!
    #! ELSE
    Sbr_FnNotFound " ${ 0 } ReStart"
    EXIT="" ### force "Fall Thru" if BASH call ${0} fails
    #!
} #
export -f Sbr_Cbx_ReStart
#!
#!
#(: ======================================================
#(: fx Sbr_Cbx_ReBoot
#(: called by: C3C_Reboot et al 
#(: param:
#(: purpose:
#(: calls:
#(: called by: C3C, ARB, E3R
#(: method:
#(: sends:
#(: rationale:
#(: note :
#(:  "PEX" distros do not handle "ReBoot / PowerOff" properly, 
#(:  therefodre Cbx_ReBoot should test for "PEX" flag, call ReStart. 
#(:
#! ###_CBX_################################################
function Sbr_Cbx_ReBoot() {   # ACTIVE  Simple ReBoot 
    n=0
    #!
    if [ "${gcSysChkPexFlg}" = "true" ] ; then 	#! PEX is Active, OS does NOT handle ReBoot !
		n=0 #! PEX should only run ReStart
		Sbr_Splash_It "PEX  Cbx_ReSTART "  "yellow" "red" "5" #! -D!---
		Sbr_Cbx_ReStart
    else
		Sbr_Splash_It "Cbx S.I.M.P.L.E. _ReBoot "  "yellow" "green" "3" #! -D!---
		Sbr_ClearFlagsSystem  ""  #: clear lockfile, etc.
		Sbr_Sys_SMP      #! Sbr_Sys_SMP  #! Save-2-Flash 
		Sbr_Sys_Set_PEX	 #! setup the ReBoot wmreboot
        ${gcDistroReboot}   #! possibly "'wmreboot'"  Distro may differ. 
    fi
    EXIT="" ### force "Fall Thru" if BASH call ${0} fails
    #! 
} #
export -f Sbr_Cbx_ReBoot
#!
#!
#(: ======================================================================
#(: fx Sbr_Cbx_ReQuit
#(: called by: CBX_ReQuit Simple PowerOff
#(: param:
#(: purpose:
#(: calls:
#(: called by: Simple ReQuit on Main System Screen
#(: method:
#(: sends:
#(: rationale:
#(: note :
#(:  "PEX" distros do not handle "ReBoot / PowerOff" properly, 
#(:  therefodre Cbx_ReBoot should test for "PEX" flag, call ReStart. 
#(:
#! ###_CBX_################################################
function Sbr_Cbx_ReQuit() {   # ACTIVE   Simple PowerOff
    n=0
    #!
    if [ "${gcSysChkPexFlg}" = "true" ] ; then 	#! PEX is Active, OS does NOT handle ReBoot !
		n=0
		Sbr_Splash_It "PEX Cbx_ReSTART "  "yellow" "red" "5" #! -D!---
		Sbr_Cbx_ReStart
		#(:  "PEX" distros do not handle "ReBoot / PowerOff" properly, 
		#(:  therefodre Cbx_ReBoot should test for "PEX" flag, call ReStart. 
	else
		Sbr_Splash_It "Cbx S.I.M.P.L.E. BASH poweroff "  "yellow" "green" "1" #! -D!---
		Sbr_ClearFlagsSystem  ""  #: clear lockfile, etc.
        Sbr_Sys_Set_PEX
		Sbr_Sys_SMP      #! Sbr_Sys_SMP  #! Save-2-Flash 
        poweroff            #! ReQUIT___BASH_poweroff_
    fi
    EXIT="" ### force "Fall Thru" if BASH call ${0} fails
} #
export -f Sbr_Cbx_ReQuit
#!
#(: ======================================================================
#(: fx Sbr_Cbx_RePowerOff
#(: called by: CBX_ReQuit Simple PowerOff
#(: param:
#(: purpose:
#(: calls:
#(: called by: Simple ReQuit on Main System Screen
#(: method:
#(: sends:
#(: rationale:
#(: note :
#(:  "PEX" distros do not handle "ReBoot / PowerOff" properly, 
#(:  therefodre Cbx_ReBoot should test for "PEX" flag, call ReStart. 
#(:
#! ###_CBX_################################################
function Sbr_Cbx_RePowerOff() {   # ACTIVE   imple PowerOff
    n=0
    #!
    if [ "${gcSysChkPexFlg}" = "true" ] ; then  #! PEX is Active, OS does NOT handle ReBoot !
		n-0
		Sbr_Splash_It "PEX  Cbx_ReSTART "  "yellow" "red" "5" #! -D!---
		Sbr_Cbx_ReStart
		#(:  "PEX" distros do not handle "ReBoot / PowerOff" properly, 
		#(:  therefodre Cbx_ReBoot should test for "PEX" flag, call ReStart. 
    else
		Sbr_Splash_It "Cbx  S.I.M.P.L.E. BASH _PowerOff "  "yellow" "green" "1" #! -D!---
		Sbr_ClearFlagsSystem  ""  #: clear lockfile, etc. 
        Sbr_Sys_Set_PEX
		Sbr_Sys_SMP      #! Sbr_Sys_SMP  #! Save-2-Flash 
        poweroff   #! RePowerOFF___absolute_for_popping_stack___BASH_poweroff
    fi
    EXIT="" ### force "Fall Thru" if BASH call ${0} fails
    #!
} #
export -f Sbr_Cbx_RePowerOff
#!
#!
#(: ======================================================================
#(: fx Sbr_Cbx_ReHalt
#(: called by: simple CBX_ReHalt
#(: param:
#(: purpose:
#(: calls:
#(: called by: Simple ReHalt on Main System Screen 
#(: method:
#(: sends:
#(: rationale:
#(: note :
#(:
#! ###_CBX_###########################################################
function Sbr_Cbx_ReHalt() {   # ACTIVE  Simple Absolute KILL system
    #! for Check-Box ARB_ReHalt___  absolute_for_killing_stack ___ 
    n=0
    #!
    if [ "${gcSysChkPexFlg}" = "true" ] ; then  #! PEX is Active, OS does NOT handle ReBoot !
		n-0
		Sbr_Splash_It "Cbx PEX _ReSTART "  "yellow" "red" "5" #! -D!---
		Sbr_Cbx_ReStart
    else
		Sbr_Splash_It "Cbx S.I.M.P.L.E. BASH _HALT "  "yellow" "green" "3" #! -D!---
		Sbr_ClearFlagsSystem  ""  #: clear lockfile, etc,  L:3551, L:4845
        Sbr_Sys_Set_PEX
		Sbr_Sys_SMP      #! Sbr_Sys_SMP  #! Save-2-Flash 
        halt
    fi
    #!
} #
export -f Sbr_Cbx_ReHalt
#!
#(: ======================================================================
#(: fx Sbr_Cbx_ReHalt
#(: called by: simple CBX_ReHalt
#(: param:
#(: purpose:
#(: calls:
#(: called by: Simple ReHalt on Main System Screen 
#(: method:
#(: sends:
#(: rationale:
#(: note :
#(:
#! ###_CBX_################################################
function Sbr_Cbx_Clear_Restart() {   # NOT ACTIVE
    #! for Check-Box ARB_ReHalt___  absolute_for_killing_stack ___ 
    n=0
    lcMsgStr="Sbr_Sbr_Cbx_Clear_Restart_"
    Sbr_Splash_Flip3  ${lcMsgStr} ".3"  &&     sleep 1
    #!
    Sbr_ClearFlagsSystem 0      ### clear lockfile so that ReStart can run.
    Sbr_Cbx_ReStart 1    # display and no wait
    #!
    EXIT="" ### force "Fall Thru" if BASH call fails
    exit 1      #! ## "exit 1' , error , should terminate this program quietly
} #
export -f Sbr_Cbx_Clear_Restart
#!
#!
