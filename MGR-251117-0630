#!=====================================================
#! ### begin Fall-Thru  "initz" 
#!=====================================================
#!
#!
#! --- MAIN () 
#! deprecate this config routine 
if [ -f ${gcMgrArbCfgFn} ] ; then   #! "menu config"  
    n=0
    gcMgrArbCfgFn="/root/my-applications/bin/C3M_Config_Arb"   # "menu config"
    gcMGRConfigArbIdd=`cat ${gcMgrArbCfgFn}`   #! "menu config"
else    #  NO Config value
    n=0
    touch ${gcMgrArbCfgFn}  #! "menu config"
    gcMGRConfigArbIdd="---" #! "menu config"  
fi  #
#! 
#! --- MAIN () 
. /etc/rc.d/PUPSTATE
export PUPMODE
#!
#! --- MAIN () 
#!
#!   : Sys_Case_PupMode 	#! called Sbr must precede . 
case $PUPMODE in
    12|13) ok=1 ;;  
    *) ERRORMSG="Not PupMode !  only (12|13)" 
        sleep 3
            #!exit 1
            Sbr_Cbx_ReQuit 	#! must follow the Called Sbr_Cbx_ReQuit . 
        ;;
esac
#!
#! --- MAIN () 
#! Find and verify path of currently mounted saveDATA entity. Could be "/saveDATA" or "saveDATA.4fs" . 
export gcSaveDataName="`echo $PUPSAVE | cut -f 3 -d ","`"    
if [ ! -e "/mnt/home${gcSaveDataName}" ]; then   # Error is Trapped 
    /usr/lib/gtkdialog/box_ok "Pupsave Backup" error "'/mnt/home${gcSaveDataName}' does not exist!"   ### ShowError
    sleep 5
    Sbr_ClearFlagsSystem 0      ### clear lockfile so that ReStart can run.
    exit 1
fi
#! --- MAIN () 
#! Case ERRORMSG catch-all
if [ "$ERRORMSG" ] ; then
    Sbr_ClearFlagsSystem 0      ### clear lockfile so that ReStart can run.
    echo "$ERRORMSG"
    ERRORMSG="<b><span foreground='"'red'"'>${ERRORMSG}</span></b>"
    exit 1
fi # end: if [ "$ERRORMSG" ] 
#--------------------------------------------------------------------
export PBKP_PUPSAVE_PATH="/mnt/home${gcSaveDataName}"
